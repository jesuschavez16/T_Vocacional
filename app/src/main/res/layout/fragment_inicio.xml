<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".inicioFragment"
    android:background="@drawable/fondo02">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"

        android:layout_marginTop="100dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/btn_basico"
            android:layout_width="140dp"
            android:layout_height="140dp"
            android:layout_marginBottom="40dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/tes_basico" />

        <ImageView
            android:id="@+id/btncompleto"
            android:layout_width="140dp"
            android:layout_height="140dp"
            android:layout_marginBottom="40dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/tes_completo" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="281dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="50dp"
        android:text="@string/Instruccion3"
        android:textColor="#ecf0f1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="220dp"
        android:text="@string/res_test"
        android:textColor="#ecf0f1"
        android:textSize="16sp" />

</FrameLayout>